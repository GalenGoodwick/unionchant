{
  "crons": [
    {
      "path": "/api/cron/process-timers",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron/ai-orchestrator",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 3 * * *"
    }
  ],
  "functions": {
    "api/auth/anonymous.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/auth/anonymous",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
